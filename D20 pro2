import java.util.*;
public class it1{
public static void main(String[] args) {

ArrayList<Integer> h1=new ArrayList<Integer>();
h1.add(30);
h1.add(40);
h1.add(15);
h1.add(20);
h1.add(25);
h1.add(7);
h1.add(99);
Iterator<Integer> z=h1.iterator();
while(z.hasNext()) 
{
  {
if(z.next()<=20)
z.remove();
}
System.out.println(h1);
}
}
}

Output:

[30, 40, 15, 20, 25, 7, 99]
[30, 40, 15, 20, 25, 7, 99]
[30, 40, 20, 25, 7, 99]
[30, 40, 25, 7, 99]
[30, 40, 25, 7, 99]
[30, 40, 25, 99]
[30, 40, 25, 99]
